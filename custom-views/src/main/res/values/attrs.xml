<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WeatherField">
        <attr name="fieldLabel" format="string" />
        <attr name="fieldValue" format="string" />
        <attr name="unit" format="enum">
            <enum name="none" value="0" />
            <enum name="percent" value="1" />
            <enum name="speed" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="DownloadStateView">
        <attr name="downloadState" format="enum">
            <enum name="success" value="0" />
            <enum name="loading" value="1" />
            <enum name="error" value="1" />
        </attr>
    </declare-styleable>
</resources>